{
    "name": "snapform-platform",
    "version": "1.0.0",
    "description": "Snapform Platform",
    "private": true,
    "author": "Garrick Thurston",
    "scripts": {
        "start": "webpack-dev-server --watch --open --hot --config webpack.dev.config.js",
        "build": "webpack --config webpack.prod.config.js",
        "build:dev": "webpack --config webpack.dev.config.js",
        "test:unit": "mocha --opts testFramework/mocha.opts \"./src/**/*.spec.js*\"",
        "test:unit:watch": "npm run test:unit -- --watch",
        "test:cover": "cross-env NODE_ENV=test nyc npm run test:unit",
        "test:cover:report": "nyc report --reporter=cobertura"
    },
    "nyc": {
        "extension": [
            ".jsx",
            ".js"
        ],
        "require": [
            "@babel/register",
            "@babel/polyfill"
        ],
        "all": true,
        "check-coverage": true,
        "lines": 35,
        "sourceMap": false,
        "instrument": false,
        "exclude": [
            "js/**",
            "src/**/*.spec.js",
            "testFramework/**",
            "dist/**",
            "examples/**",
            "*.config.js",
            "webpack.*.js",
            "src/**/index.js"
        ]
    },
    "dependencies": {
        "@babel/polyfill": "^7.7.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.26",
        "@fortawesome/free-solid-svg-icons": "^5.12.0",
        "@fortawesome/react-fontawesome": "^0.1.8",
        "axios": "^0.19.0",
        "bootstrap": "^4.4.1",
        "jquery": "^3.4.1",
        "jsonwebtoken": "^8.5.1",
        "popper.js": "^1.16.0",
        "query-string": "^6.9.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-modal": "^3.11.1",
        "react-router-dom": "^5.1.2",
        "tether": "^1.4.7"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
        "@babel/plugin-syntax-dynamic-import": "^7.7.4",
        "@babel/plugin-transform-modules-umd": "^7.7.4",
        "@babel/preset-env": "^7.7.6",
        "@babel/preset-react": "^7.7.4",
        "@babel/register": "^7.7.4",
        "autoprefixer": "^9.7.3",
        "babel-eslint": "^8.2.5",
        "babel-loader": "^8.0.5",
        "babel-plugin-add-module-exports": "^1.0.2",
        "babel-plugin-dynamic-import-node": "^2.3.0",
        "babel-plugin-istanbul": "^5.2.0",
        "babel-plugin-webpack-alias": "^2.1.2",
        "chai": "^4.2.0",
        "clean-webpack-plugin": "^1.0.1",
        "cross-env": "^5.2.1",
        "css-loader": "^2.1.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-import-resolver-webpack": "^0.12.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^2.3.0",
        "file-loader": "^5.0.2",
        "html-webpack-plugin": "^3.2.0",
        "ignore-styles": "^5.0.1",
        "jsdom": "^15.2.1",
        "json-loader": "^0.5.7",
        "mini-css-extract-plugin": "^0.9.0",
        "mocha": "^6.2.2",
        "mocha-junit-reporter": "^1.23.1",
        "mocha-multi-reporters": "^1.1.7",
        "node-sass": "^7.0.0",
        "nyc": "^15.0.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^3.0.0",
        "sass-loader": "^8.0.0",
        "sinon": "^8.0.1",
        "style-loader": "^1.1.2",
        "svg-inline-loader": "^0.8.0",
        "terser-webpack-plugin": "^2.3.1",
        "text-loader": "0.0.1",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.4",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1",
        "webpack-merge": "^4.2.2",
        "webpack-visualizer-plugin": "^0.1.11"
    }
}
